{
  "name": "mindshore-project-catalog",
  "version": "1.0.0",
  "description": "Production-ready Project Catalog for MindShore - showcasing Data & AI projects with client anonymization",
  "type": "module",
  "scripts": {
    "predev": "kill-port 5173 || true",
    "dev": "vite --port 5173 --strictPort",
    "build": "tsc -b && vite build",
    "preview": "vite preview --port 5173 --strictPort",
    "ingest": "tsx scripts/ingest-docx.ts",
    "test": "playwright test",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "format": "prettier --write .",
    "format:check": "prettier --check ."
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "mammoth": "^1.6.0",
    "lucide-react": "^0.291.0"
  },
  "devDependencies": {
    "@types/react": "^18.2.37",
    "@types/react-dom": "^18.2.15",
    "@types/node": "^22.0.0",
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^7.0.0",
    "@vitejs/plugin-react": "^4.1.0",
    "autoprefixer": "^10.4.16",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.4",
    "kill-port": "^2.0.1",
    "postcss": "^8.4.31",
    "prettier": "^3.0.3",
    "playwright": "^1.40.0",
    "tailwindcss": "^3.3.5",
    "tsx": "^4.0.0",
    "typescript": "^5.2.2",
    "vite": "^5.0.0"
  },
  "engines": {
    "node": ">=18"
  },
  "keywords": [
    "mindshore",
    "project-catalog",
    "data-ai",
    "react",
    "vite",
    "typescript",
    "tailwind"
  ],
  "author": "MindShore",
  "license": "MIT"
}